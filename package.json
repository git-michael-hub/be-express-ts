{
  "name": "be-expressjs-ts",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "node dist/main.js",
    "dev": "nodemon --watch src --exec ts-node ./src/main.ts",
    "build": "tsc",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate ./src/migrations/CreateTables -d ./src/data-source.ts",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d ./src/data-source.ts",
    "test": "NODE_ENV=test jest --silent --runInBand",
    "test:coverage": "NODE_ENV=test jest --silent --coverage --runInBand",
    "test:ui": "NODE_ENV=test jest --silent --reporters=jest-html-reporter --runInBand",
    "test:watch": "NODE_ENV=test jest --watch --silent --runInBand",
    "test:debug": "NODE_ENV=test jest --detectOpenHandles --forceExit --silent --runInBand",
    "seed:tasks": "ts-node src/seeder/task.seed.ts seed",
    "clear:tasks": "ts-node src/seeder/task.seed.ts clear",
    "stats:tasks": "ts-node src/seeder/task.seed.ts stats",
    "seed:tasks:force": "ts-node src/seeder/task.seed.ts seed --force",
    "seed:users": "ts-node src/seeder/user.seed.ts seed",
    "clear:users": "ts-node src/seeder/user.seed.ts clear",
    "stats:users": "ts-node src/seeder/user.seed.ts stats",
    "verify:users": "ts-node src/seeder/user.seed.ts verify",
    "test-user": "ts-node src/seeder/user.seed.ts test-user",
    "seed:users:force": "ts-node src/seeder/user.seed.ts seed --force",
    "seed:projects": "ts-node src/seeder/project.seed.ts seed",
    "clear:projects": "ts-node src/seeder/project.seed.ts clear",
    "stats:projects": "ts-node src/seeder/project.seed.ts stats",
    "validate:projects": "ts-node src/seeder/project.seed.ts validate",
    "test-project": "ts-node src/seeder/project.seed.ts test-project",
    "seed:projects:force": "ts-node src/seeder/project.seed.ts seed --force"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^7.0.6",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.20",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.10.2",
    "@types/nodemailer": "^7.0.1",
    "@types/supertest": "^6.0.3",
    "jest": "^30.0.5",
    "jest-html-reporter": "^4.3.0",
    "nodemon": "^3.1.7",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typeorm-ts-node-commonjs": "^0.3.20",
    "typeorm-ts-node-esm": "^0.3.20",
    "typescript": "^5.7.2"
  }
}
